# Example .env - copy to backend/.env and fill with real values (DO NOT COMMIT the real .env)

# Security
JWT_SECRET=REPLACE_WITH_A_SECURE_RANDOM_VALUE

# Database
DB_HOST=127.0.0.1
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=your_db_name
DB_PORT=3306

# Server
PORT=8080
NODE_ENV=production

# Email / OAuth (if used)
EMAIL_HOST=your.email@example.com
CLIENT_ID=your_google_oauth_client_id
CLIENT_SECRET=your_google_oauth_client_secret
REDIRECT_URI=https://yourapp.example.com/oauth/callback
OAUTH_REFRESH_TOKEN=replace_with_refresh_token

# GCS
GCS_BUCKET=your_gcs_bucket_name
GCS_KEY_PATH=backend/gcs-key.json # keep local and do not commit
